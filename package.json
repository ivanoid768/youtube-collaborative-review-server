{
  "name": "youtube_collaborative_review_server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ivanoid768/youtube-collaborative-review-server.git",
  "author": "ivanoid256",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config ./src/index.ts",
    "dev-tools": "npm-run-all -p eslint-watch prettier-watch",
    "prettier-watch": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-format-all": "prettier --write src/**/*.ts",
    "eslint-watch": "esw -w \"src/**/*.ts\" --ext .ts",
    "eslint-fix": "eslint \"src/**/*.ts\" --ext .ts --fix",
    "build": "tsc",
    "eslint": "eslint \"src/**/*.ts\" --ext .ts -o eslint.xml -f checkstyle",
    "debug": "ts-node-dev --inspect --respawn --transpileOnly -r dotenv/config ./src/index.ts",
    "sonar": "sonar-scanner"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.3",
    "kurento-client": "^6.14.0",
    "lodash": "^4.17.15",
    "nanoid": "^3.1.10",
    "peer": "^0.5.3",
    "socket.io": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/kurento-client": "^6.14.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.13.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-watch": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.0",
    "prettier": "^2.0.4",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "3.8.3"
  }
}
